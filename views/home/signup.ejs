<%- include('../_partial/_header'); -%>

<form class="form-signin" method="POST" action="/signup">
    <h1 class="h3 mb-3 font-weight-normal">Sign Up</h1>
    <input type="text" class="form-control" placeholder="Username" name="username" aria-describedby="basic-addon1" autofocus>
    <br>
    <input type="email" class="form-control" placeholder="Email address" name="email" aria-describedby="basic-addon1" autofocus>
    <br>
    <input type="password" name="password" class="form-control" placeholder="Password" aria-describedby="basic-addon1" autofocus>
    <br>
    <input type="password" name="repassword" class="form-control" placeholder="Re-password" aria-describedby="basic-addon1" autofocus>
    <br>
    <input type="submit" class="tn btn-lg btn-primary btn-block" value="Sign Up" />

</form>

 
<%- include('../_partial/_footer'); -%>